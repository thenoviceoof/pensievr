{
    "name": "Pensievr-plugin",
    "version": "0.1",
    "manifest_version": 2,
    "description": "A write-only twitter-like diary",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://pensievr.appspot.com",
        "geolocation",
        "localstorage"
    ],
    "web_accessible_resources": [
        "script.js"
    ]
}
