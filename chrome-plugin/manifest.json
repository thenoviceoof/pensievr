{
    "name": "Pensievr-plugin",
    "version": "0.1",
    "manifest_version": 2,
    "description": "A write-only twitter-like diary",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "exclude_matches": ["https://pensievr.appspot.com/*"],
            "js": ["jquery-1.7.2.min.js", "cs.js"]
        }
    ],
    "permissions": [
        "https://pensievr.appspot.com",
        "geolocation",
        "localstorage"
    ]
}
